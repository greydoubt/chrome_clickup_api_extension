{
  "manifest_version": 2,
  "name": "Clickup API Extension",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://api.clickup.com/*"
  ],
  "web_accessible_resources": [
    "popup.html",
    "popup.js"
  ]
}
